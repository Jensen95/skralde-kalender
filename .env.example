# Copy this file to .env and fill in your values

# Cloudflare Account ID (found in Cloudflare Dashboard)
CLOUDFLARE_ACCOUNT_ID=your-account-id

# KV Namespace IDs (create these with wrangler kv:namespace create)
KV_NAMESPACE_ID=your-kv-namespace-id
KV_PREVIEW_NAMESPACE_ID=your-preview-kv-namespace-id

# Email settings
DESTINATION_EMAIL=calendar@yourdomain.com
CALENDAR_NAME="My Ice Calendar"
CALENDAR_DESCRIPTION="Calendar populated from email forwards"

# Worker settings
WORKER_NAME=ice-calendar-worker
WORKER_SUBDOMAIN=your-subdomain